{
  "name": "react-yandex-maps",
  "version": "3.0.1-canary.ef9dc35",
  "description": "Yandex.Maps API bindings for React",
  "main": "dist/react-yandex-maps.js",
  "umd:main": "dist/react-yandex-maps.umd.js",
  "module": "dist/react-yandex-maps.m.js",
  "source": "src/index.js",
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "scripts": {
    "clear": "rm -rf dist",
    "prettier": "prettier --write",
    "lint": "eslint",
    "precommit": "lint-staged",
    "build": "npm run clear && microbundle --jsx React.createElement --target web --strict true",
    "dev": "microbundle watch --jsx React.createElement --target web --strict true --compress false",
    "release": "./scripts/release.sh"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint",
      "npm run prettier",
      "git add"
    ],
    "*.{json,css,md}": [
      "npm run prettier",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gribnoysup/react-yandex-maps.git"
  },
  "peerDependencies": {
    "react": "^0.14.9 || ^15.3.0 || ^16.0.0"
  },
  "dependencies": {
    "create-react-context": "^0.2.2",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "microbundle": "^0.4.4",
    "prettier": "^1.14.2",
    "react": "^16.4.2"
  },
  "keywords": [
    "react",
    "react-component",
    "yandex",
    "maps",
    "yandex-maps"
  ],
  "author": "Sergey Petushkov <petushkov.sergey@gmail.com>",
  "license": "MIT",
  "sideEffects": false,
  "bugs": {
    "url": "https://github.com/gribnoysup/react-yandex-maps/issues"
  },
  "homepage": "https://github.com/gribnoysup/react-yandex-maps#readme",
  "directories": {
    "doc": "docs"
  }
}
