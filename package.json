  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.0",
    "microbundle": "^0.4.4",
    "prettier": "^1.11.1",
    "react": "^16.2.0"
  },
  "keywords": ["react", "react-component", "yandex", "maps", "yandex-maps"],
